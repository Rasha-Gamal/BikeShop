<!DOCTYPE html>

<html>
<head>
    
    <title>@ViewBag.Title</title>

    
    <link href="~/content/css/bootstrap.css" rel='stylesheet' type='text/css' />
    <!-- jQuery (Bootstrap's JavaScript plugins) -->
    <script src="~/content/js/jquery.min.js"></script>
    <!-- Custom Theme files -->
    <link href="~/content/css/style.css" rel="stylesheet" type="text/css" media="all" />
    <!-- Custom Theme files -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Bike-shop Responsive web template, Bootstrap Web Templates, Flat Web Templates, Andriod Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyErricsson, Motorola web design" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!--webfont-->
    <link href='http://fonts.googleapis.com/css?family=Roboto:500,900,100,300,700,400' rel='stylesheet' type='text/css'>
    <!--webfont-->
    <!-- dropdown -->
    <script src="~/content/js/jquery.easydropdown.js"></script>
    <link href="~/content/css/nav.css" rel="stylesheet" type="text/css" media="all" />
    <script src="~/content/js/scripts.js" type="text/javascript"></script>
    <!--js-->
    <!---- start-smoth-scrolling---->
    <script type="text/javascript" src="~/content/js/move-top.js"></script>
    <script type="text/javascript" src="~/content/js/easing.js"></script>
    <script type="text/javascript">
        jQuery(document).ready(function ($) {
            $(".scroll").click(function (event) {
                event.preventDefault();
                $('html,body').animate({ scrollTop: $(this.hash).offset().top }, 900);
            });
        });
    </script>
    <!---- start-smoth-scrolling---->
</head>
<body>
    <!--banner-->
    <script src="~/content/js/responsiveslides.min.js"></script>
    <script>
        $(function () {
            $("#slider").responsiveSlides({
                auto: true,
                nav: true,
                speed: 500,
                namespace: "callbacks",
                pager: true,
            });
        });
    </script>
    <div class="banner-bg banner-bg1">
        <div class="container">
            <div class="header">
                <div class="logo">
                    <a asp-controller="ShowTemplate" asp-action="Index"><img src="~/content/images/logo.png" alt="" /></a>
                </div>
                <div class="top-nav">
                    <label class="mobile_menu" for="mobile_menu">
                        <span>Menu</span>
                    </label>
                    <input id="mobile_menu" type="checkbox">
                    <ul class="nav">
                        <li class="dropdown1">
                            <a asp-controller="ShowTemplate" asp-action="Bicycles">BICYCLES</a>
                            <ul class="dropdown2">
                                <li><a asp-controller="ShowTemplate" asp-action="Bicycles">FIXED / SINGLE SPEED</a></li>
                                <li><a asp-controller="ShowTemplate" asp-action="Bicycles">CITY BIKES</a></li>
                                <li><a asp-controller="ShowTemplate" asp-action="Bicycles">PREMIMUN SERIES</a></li>
                            </ul>
                        </li>
                        <li class="dropdown1">
                            <a asp-controller="ShowTemplate" asp-action="Parts">PARTS</a>
                            <ul class="dropdown2">
                                <li><a asp-controller="ShowTemplate" asp-action="Parts" >CHAINS</a></li>
                                <li><a asp-controller="ShowTemplate" asp-action="Parts">TUBES</a></li>
                                <li><a asp-controller="ShowTemplate" asp-action="Parts">TIRES</a></li>
                                <li><a asp-controller="ShowTemplate" asp-action="Parts">DISC BREAKS</a></li>
                            </ul>
                        </li>
                        <li class="dropdown1">
                            <a asp-controller="ShowTemplate" asp-action= "Accessories">ACCESSORIES</a>
                            <ul class="dropdown2">
                                <li><a asp-controller="ShowTemplate" asp-action= "Accessories">LOCKS</a></li>
                                <li><a asp-controller="ShowTemplate" asp-action= "Accessories">HELMETS</a></li>
                                <li><a asp-controller="ShowTemplate" asp-action= "Accessories">ARM COVERS</a></li>
                                <li><a asp-controller="ShowTemplate" asp-action= "Accessories">JERSEYS</a></li>
                            </ul>
                        </li>
                        <li class="dropdown1">
                            <a asp-controller="ShowTemplate" asp-action ="Extras">EXTRAS</a>
                            <ul class="dropdown2">
                                <li><a asp-controller="ShowTemplate" asp-action ="Extras">CLASSIC BELL</a></li>
                                <li><a asp-controller="ShowTemplate" asp-action ="Extras">BOTTLE CAGE</a></li>
                                <li><a asp-controller="ShowTemplate" asp-action ="Extras">TRUCK GRIP</a></li>
                            </ul>
                        </li>
                        <a class="shop" asp-controller="ShowTemplate" asp-action="Cart"><img src="~/content/images/cart.png" alt="" /></a>
                    </ul>
                </div>
                <div class="clearfix"></div>
            </div>
        </div>
        <div class="caption">
            <div class="slider">
                <div class="callbacks_container">
                    <ul class="rslides" id="slider">
                        <li><h1>HANDMADE BICYCLE</h1></li>
                        <li><h1>SPEED BICYCLE</h1></li>
                        <li><h1>MOUINTAIN BICYCLE</h1></li>
                    </ul>
                    <p>You <span>create</span> the <span>journey,</span> we supply the <span>parts</span></p>
                    <a class="morebtn" asp-controller="ShowTemplate" asp-action ="Bicycles">SHOP BIKES</a>
                </div>
            </div>
        </div>
        <div class="dwn">
            <a class="scroll" href="#cate"><img src="~/content/images/scroll.png" alt="" /></a>
        </div>
    </div>
    <!--/banner-->

    <div>
        @RenderBody()
    </div>


    <div class="contact">
        <div class="container">
            <h3>CONTACT US</h3>
            <p>Please contact us for all inquiries and purchase options.</p>
            <form>
                <input type="text" placeholder="NAME" required="">
                <input type="text" placeholder="SURNAME" required="">
                <input class="user" type="text" placeholder="USER@DOMAIN.COM" required=""><br>
                <textarea placeholder="MESSAGE"></textarea>
                <input type="submit" value="SEND">
            </form>
        </div>
    </div>
    <!---->
    <div class="footer">
        <div class="container wrap">
            <div class="logo2">
                <a asp-controller="ShowTemplate" asp-action="Index"><img src="~/content/images/logo2.png" alt="" /></a>
            </div>
            <div class="ftr-menu">
                <ul>
                    <li><a asp-controller="ShowTemplate" asp-action="Bicycles">BICYCLES</a></li>
                    <li><a asp-controller="ShowTemplate" asp-action="Parts">PARTS</a></li>
                    <li><a asp-controller="ShowTemplate" asp-action="Accessories">ACCESSORIES</a></li>
                    <li><a asp-controller="ShowTemplate" asp-action="Extras">EXTRAS</a></li>
                </ul>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
    <!---->
</body>
</html>
